<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <SolidColorBrush x:Key="{x:Static SystemColors.WindowTextBrushKey}" Color="White" />
  <SolidColorBrush x:Key="{x:Static SystemColors.WindowTextBrush}" Color="White" />
  <SolidColorBrush x:Key="{x:Static SystemColors.MenuTextBrush}" Color="White" />
  <SolidColorBrush x:Key="{x:Static SystemColors.InfoTextBrush}" Color="White" />
  <SolidColorBrush x:Key="{x:Static SystemColors.ControlTextBrush}" Color="White" />
  <SolidColorBrush x:Key="{x:Static SystemColors.ActiveBorderBrush}" Color="Magenta" />
  <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrush}" Color="DeepSkyBlue" />
  <SolidColorBrush x:Key="{x:Static SystemColors.GrayTextBrushKey}" Color="Yellow" />
  <SolidColorBrush x:Key="{x:Static SystemColors.WindowBrushKey}" Color="Chartreuse" />
  <SolidColorBrush x:Key="{x:Static SystemColors.ControlBrushKey}" Color="HotPink" />
</ResourceDictionary>